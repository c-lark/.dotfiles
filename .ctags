--langdef=vhdl
--langmap=vhdl:.vhd,.VHD
--regex-vhdl=/^[ \t]*entity[ \t]*([a-zA-Z0-9_]+)/\1/e,entity/
--regex-vhdl=/^[ \t]*architecture[ \t]*([a-zA-Z0-9_]+)[ \t]*of[ \t]*([a-zA-Z0-9_]+)/\1 of \2/a,architecture/
--regex-vhdl=/^[ \t]*constant[ \t]*([a-zA-Z0-9_]+)/\1/c,constant/
--regex-vhdl=/^[ \t]*package[ \t]*([a-zA-Z0-9_]+)/\1/p,package/
--regex-vhdl=/^[ \t]*package[ \t]*body[ \t]*([a-zA-Z0-9_]+)/\1/b,package_body/
--regex-vhdl=/^[ \t]*type[ \t]*([a-zA-Z0-9_]+)/\1/t,type/
--regex-vhdl=/^[ \t]*signal[ \t]*([a-zA-Z0-9_]+)/\1/s,signal/
--regex-vhdl=/^[ \t]*([a-zA-Z0-9_]+[ \t]*:)*[ \t]*process/\1/r,process/
--regex-vhdl=/^[ \t]*([a-zA-Z0-9_]+[ \t]*:)*[ \t]*block/\1/l,block/
--regex-vhdl=/^[ \t]*function[ \t]*([a-zA-Z0-9_]+)/\1/u,function/
--regex-vhdl=/^[ \t]*procedure[ \t]*([a-zA-Z0-9_]+)/\1/u,procedure/
--regex-vhdl=/^[ \t]*subtype[ \t]*([a-zA-Z0-9_]+)/\1/y,subtype/
--regex-vhdl=/^[ \t]*configuration[ \t]*([a-zA-Z0-9_]+)/\1/n,configuration/

--langdef=rubytest
--langmap=rubytest:.rb
--regex-rubytest=/test \"(.*)\" do/\1/f/
